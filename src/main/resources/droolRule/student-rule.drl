package droolRule ;

import org.slf4j.Logger
import org.slf4j.LoggerFactory
import com.drools.rules.entity.*;
dialect  "java"

rule "checkAge"
    salience 100
    when $stu : StudentFact(age > 18)
    then
        final Logger LOGGER = LoggerFactory.getLogger("已经成年↓") ;
        LOGGER.info("***"+$stu.getName()+"已经成年\n");
end